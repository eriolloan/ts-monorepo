{
    "preset": "ts-jest/presets/js-with-ts-esm",
    "globals": {
        "ts-jest": {
            "useESM": true
        }
    },
    "extensionsToTreatAsEsm": [
        ".ts"
    ],
    "testMatch": [
        "<rootDir>/src/**/*.test.ts"
    ],
    "transform": {},
    "moduleNameMapper": {
        "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transformIgnorePatterns": [
        "/node_modules/"
    ]
}